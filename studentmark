class StudentMarks{
  public static void main(String[]args){
  double odiyaMarks        =80.5;
  double englishMarks      =55.6;
  double hindiMarks        =78.9;
  double mathMarks         =100;
  double scienceMarks      =100;
  double socialscienceMarks=100;
  double totalmarks=odiyaMarks+englishMarks+hindiMarks+mathMarks+scienceMarks+socialscienceMarks;

  System.out.println(totalmarks);
}
 }
